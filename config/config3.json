{
  "version": 1,
  "basic": {
    // basic config
    "timestep": 1,
    "timeUnit": "hour",
    "timeEpoch": 1, // 运行几次
    "outputPath": "output/"
  },
  "instance": [
    {
      // instance declare
      "name": "dem",
      "field": "dem",
      "init": {
        // init declare
        "use": "define", // define | no | copy
        "define": {
          // define declare
          "method": "txt2Raster",
          "config": {
            "inFile": true,
            "inFilePath": "data/demtest.txt"
          },
          "data": null
        }
      }
    },
    {
      "name": "water",
      "field": "rain",
      "init": {
        "use": "no"
      }
    },
    {
      "name": "sumoNetwork",
      "field": "tempFile",
      "init": {
        "use": "define",
        "define": {
          "method": "fileList",
          "config": {},
          "data": {
            "network": "data/test165.net.xml"
          }
        }
      }
    },
    {
      "name": "sumoRoute",
      "field": "tempFile",
      "init": {
        "use": "define",
        "define": {
          "method": "fileList",
          "config": {},
          "data": {
            "route": "data/si3.rou.xml"
          }
        }
      }
    },
    {
      "name": "sumoUni",
      "field": "sumoUni",
      "init": {
        // init declare
        "use": "define",
        "define": {
          // define declare
          "method": "json",
          "config": {
            "outListConf": true,
            "inFile": true,
            "inFilePath": "data/sumo.json"
          },
          "data": null
        }
      }
    },
    {
      "name": "lisUni",
      "field": "lisfloodUni",
      "init": {
        // init declare
        "use": "define",
        "define": {
          // define declare
          "method": "json",
          "config": {
            "outListConf": true,
            "inFile": true,
            "inFilePath": "data/test.json"
          },
          "data": null
        }
      }
    }
  ],
  "link": [
    {
      // link declare
      "module": "sumo",
      "timeInter": 1, // 每一个timestep运行一次
      "input": [
        // link input
        {
          // input declare
          "into": "uniSumo",
          "use": "instance", // instance | define
          "instance": "sumoUni" //fieldname
        },
        {
          "into": "network",
          "use": "instance", // instance | define
          "instance": "sumoNetwork" //fieldname
        },
        {
          "into": "routeFiles",
          "use": "instance", // instance | define
          "instance": "sumoRoute" //fieldname
        }
      ],
      "output": [],
      "recordInside": [
        {
          "catch": "files",
          "method": "file",
          "config": { "all": true },
          "onlyrun": true
        }
      ],
      "record": []
    }
  ]
}
