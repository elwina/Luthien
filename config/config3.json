{
  "version": 1,
  "basic": {
    // basic config
    "timestep": 1,
    "timeUnit": "hour",
    "timeEpoch": 1, // 运行几次
    "outputPath": "output/"
  },
  "instance": [
    {
      "name": "dem",
      "field": "dem",
      "init": {
        "use": "define", // define | no | copy
        "define": {
          "method": "txt2Raster",
          "config": {
            "inFile": true,
            "inFilePath": "data/4326test.txt"
          },
          "data": null
        }
      }
    },
    {
      "name": "water",
      "field": "rain",
      "init": {
        "use": "no"
      }
    },
    {
      "name": "sumoNetwork",
      "field": "sumoNet",
      "init": {
        "use": "define",
        "define": {
          "method": "fileList",
          "config": {},
          "data": { "sumoNet": "data/test165.net.xml" }
        }
      }
    },
    {
      "name": "road",
      "field": "road",
      "init": {
        "use": "define",
        "define": {
          "method": "sumoNet2Road",
          "config": { "filepath": "data/test165.net.xml" },
          "data": {}
        }
      }
    },
    {
      "name": "sumoRoute",
      "field": "tempFile",
      "init": {
        "use": "define",
        "define": {
          "method": "fileList",
          "config": {},
          "data": {
            "route": "data/si3.rou.xml"
          }
        }
      }
    },
    {
      "name": "vssFile",
      "field": "tempFile",
      "init": { "use": "no" }
    },
    {
      "name": "sumoUni",
      "field": "sumoUni",
      "init": {
        "use": "define",
        "define": {
          "method": "json",
          "config": {
            "outListConf": true,
            "inFile": true,
            "inFilePath": "data/sumo.json"
          },
          "data": null
        }
      }
    },
    {
      "name": "lisUni",
      "field": "lisfloodUni",
      "init": {
        "use": "define",
        "define": {
          "method": "json",
          "config": {
            "outListConf": true,
            "inFile": true,
            "inFilePath": "data/test.json"
          },
          "data": null
        }
      }
    },
    {
      "name": "sumoSpeedUni",
      "field": "sumoSpeedUni",
      "init": {
        "use": "no",
        "define": {
          "method": "json",
          "config": {
            "outListConf": true,
            "inFile": true,
            "inFilePath": "data/test.json"
          },
          "data": null
        }
      }
    }
  ],
  "link": [
    {
      // link declare
      "module": "lisflood",
      "timeInter": 1, // 每一个timestep运行一次
      "input": [
        // link input
        {
          // input declare
          "into": "dem",
          "use": "instance", // instance | define
          "instance": "dem" //fieldname
        },
        {
          "into": "lisUni",
          "use": "instance", // instance | define
          "instance": "lisUni" //fieldname
        }
      ],
      "output": [
        {
          // output action declare
          "catch": "water",
          "put": "water",
          "timeInter": 1 //TODO 每n个timestep输出到instance一次
        }
      ],
      "recordInside": [
        {
          "catch": "files",
          "method": "file",
          "config": { "all": true },
          "onlyrun": true
        }
      ],
      "record": []
    },
    {
      "module": "sumoSpeed",
      "timeInter": 1,
      "input": [
        {
          "into": "sumoNet",
          "use": "instance",
          "instance": "sumoNetwork"
        },
        {
          "into": "road",
          "use": "instance",
          "instance": "road"
        },
        {
          "into": "sumoSpeedUni",
          "use": "instance",
          "instance": "sumoSpeedUni"
        },
        {
          "into": "water",
          "use": "instance",
          "instance": "water"
        }
      ],
      "output": [
        {
          "catch": "sumoSpeed",
          "put": "sumoSpeed",
          "timeInter": 1
        }
      ],
      "recordInside": [],
      "record": []
    }
  ]
}
